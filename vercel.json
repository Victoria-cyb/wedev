{
  "version": 2,
  "builds": [
    {
      "src": "contact-form-backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/send",
      "dest": "contact-form-backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}